{
  "name": "vue-livekit-components",
  "type": "module",
  "version": "0.0.0",
  "packageManager": "pnpm@10.10.0",
  "description": "Vue LiveKit components library",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/vue-livekit-components.es.js",
      "require": "./dist/vue-livekit-components.umd.js"
    }
  },
  "main": "./dist/vue-livekit-components.umd.js",
  "module": "./dist/vue-livekit-components.es.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build",
    "playground": "pnpm --filter playground dev",
